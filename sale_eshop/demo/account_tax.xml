<odoo>

    <record id="tax_15_vat_excl" model="account.tax">
        <field name="name">15% VAT EXCL.</field>
        <field name="description">15%</field>
        <field name="amount_type">percent</field>
        <field name="amount">15</field>
        <field name="price_include" eval="False"/>
    </record>

    <record id="tax_15_vat_incl" model="account.tax">
        <field name="name">15% VAT INCL.</field>
        <field name="description">15%</field>
        <field name="amount_type">percent</field>
        <field name="amount">15</field>
        <field name="price_include" eval="True"/>
    </record>

</odoo>
